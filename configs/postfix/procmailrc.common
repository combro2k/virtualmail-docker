SHELL=/bin/sh
PATH=/bin:/usr/bin:/opt/local/bin
MAILDIR="$HOME/$DOMAIN/$USER"
DEFAULT="$MAILDIR/"
VERBOSE=OFF
LOGFILE="/home/vmail/logs/proclog-$USER"

LOG="$USER - $DOMAIN - $EXTENSION"

NL="
"
WS="    "

:0:
* ^X-Spam-Level: \*\*\*\*\*\*\*\*\*\*\*\*\*\*\*
".Junk E-mail/"

:0:
* ^X-Spam-Status: Yes
"$MAILDIR/.Junk E-mail/"

SWITCHRC="$HOME/.procmail/$DOMAIN/$USER"
